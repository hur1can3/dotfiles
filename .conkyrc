use_xft yes
xftfont Droid Sans:size=8
override_utf8_locale yes

text_buffer_size 2048
update_interval 1
total_run_times 0
double_buffer yes
no_buffers yes
net_avg_samples 1
cpu_avg_samples 1

own_window_class Conky
own_window yes
own_window_colour 3C3C3C
own_window_type override
own_window_transparent yes
own_window_hints undecorated,below,sticky,skip_taskbar,skip_pager

default_color D8D8D8
draw_borders no
border_width 1
border_margin 10
draw_graph_borders yes
draw_outline no
draw_shades no

color0 E6E6E6
color1 3465A4
color2 E6E6E6

alignment top_right
gap_x 25
gap_y 5
minimum_size 182 0
maximum_width 182

default_bar_size 60 8

imlib_cache_size 0

